    "import EasyMCP2221\n",
    "from PIL import Image, ImageDraw, ImageFont\n",
    "import time\n",
    "import math\n",
    "from datetime import datetime\n",
    "\n",
    "OLED_WIDTH = 128\n",
    "OLED_HEIGHT = 64\n",
    "OLED_I2C_ADDR = 0x3C\n",
    "\n",
    "mcp = EasyMCP2221.Device()\n",
    "\n",
    "def send_command(cmd):\n",
    "    mcp.I2C_write(OLED_I2C_ADDR, [0x00, cmd])\n",
    "\n",
    "def send_commands(cmds):\n",
    "    for cmd in cmds:\n",
    "        send_command(cmd)\n",
    "\n",
    "def send_data(data):\n",
    "    chunk_size = 60\n",
    "    for i in range(0, len(data), chunk_size):\n",
    "        chunk = data[i:i+chunk_size]\n",
    "        mcp.I2C_write(OLED_I2C_ADDR, [0x40] + list(chunk))\n",
    "\n",
    "def oled_init():\n",
    "    cmds = [\n",
    "        0xAE, 0x20, 0x00, 0xB0, 0xC8,\n",
    "        0x00, 0x10, 0x40, 0x81, 0x7F,\n",
    "        0xA1, 0xA6, 0xA8, 0x3F, 0xA4,\n",
    "        0xD3, 0x00, 0xD5, 0x80, 0xD9,\n",
    "        0xF1, 0xDA, 0x12, 0xDB, 0x40,\n",
    "        0x8D, 0x14, 0xAF\n",
    "    ]\n",
    "    send_commands(cmds)\n",
    "\n",
    "def display_image(image):\n",
    "    # Convert once to 1-bit and prepare buffer\n",
    "    bw_image = image.convert('1')\n",
    "    buffer = bytearray(OLED_WIDTH * OLED_HEIGHT // 8)\n",
    "    pixels = bw_image.load()\n",
    "    for x in range(OLED_WIDTH):\n",
    "        for page in range(OLED_HEIGHT // 8):\n",
    "            byte = 0\n",
    "            base_y = page * 8\n",
    "            for bit in range(8):\n",
    "                if pixels[x, base_y + bit]:\n",
    "                    byte |= (1 << bit)\n",
    "            buffer[x + page * OLED_WIDTH] = byte\n",
    "    for page in range(8):\n",
    "        send_command(0xB0 + page)\n",
    "        send_command(0x00)\n",
    "        send_command(0x10)\n",
    "        start = page * OLED_WIDTH\n",
    "        end = start + OLED_WIDTH\n",
    "        send_data(buffer[start:end])\n",
    "\n",
    "class ClockDisplay:\n",
    "    def __init__(self):\n",
    "        self.center_x = 36\n",
    "        self.center_y = OLED_HEIGHT // 2\n",
    "        self.radius = 28\n",
    "        self.font = ImageFont.load_default()\n",
    "        # Precompute hour mark positions (static)\n",
    "        self.hour_marks = []\n",
    "        for i in range(12):\n",
    "            angle = math.radians(i * 30 - 90)\n",
    "            x1 = self.center_x + int(self.radius * 0.85 * math.cos(angle))\n",
    "            y1 = self.center_y + int(self.radius * 0.85 * math.sin(angle))\n",
    "            x2 = self.center_x + int(self.radius * math.cos(angle))\n",
    "            y2 = self.center_y + int(self.radius * math.sin(angle))\n",
    "            self.hour_marks.append(((x1, y1), (x2, y2)))\n",
    "\n",
    "        # Create a base image with static parts (circle and hour marks)\n",
    "        self.base_image = Image.new(\"1\", (OLED_WIDTH, OLED_HEIGHT))\n",
    "        draw = ImageDraw.Draw(self.base_image)\n",
    "        draw.ellipse(\n",
    "            (self.center_x - self.radius, self.center_y - self.radius,\n",
    "             self.center_x + self.radius, self.center_y + self.radius),\n",
    "            outline=255, width=2)\n",
    "        for mark in self.hour_marks:\n",
    "            draw.line(mark[0] + mark[1], fill=255, width=2)\n",
    "\n",
    "    def draw_hands(self, now):\n",
    "        # Create a copy of base image to draw hands\n",
    "        image = self.base_image.copy()\n",
    "        draw = ImageDraw.Draw(image)\n",
    "\n",
    "        # Calculate angles (use integer math where possible)\n",
    "        hour = now.hour % 12 + now.minute / 60.0\n",
    "        minute = now.minute + now.second / 60.0\n",
    "        second = now.second\n",
    "\n",
    "        hour_angle = math.radians((hour / 12) * 360 - 90)\n",
    "        minute_angle = math.radians((minute / 60) * 360 - 90)\n",
    "        second_angle = math.radians((second / 60) * 360 - 90)\n",
    "\n",
    "        # Draw hour hand\n",
    "        hour_length = int(self.radius * 0.5)\n",
    "        hour_x = self.center_x + int(hour_length * math.cos(hour_angle))\n",
    "        hour_y = self.center_y + int(hour_length * math.sin(hour_angle))\n",
    "        draw.line((self.center_x, self.center_y, hour_x, hour_y), fill=255, width=3)\n",
    "\n",
    "        # Draw minute hand\n",
    "        minute_length = int(self.radius * 0.75)\n",
    "        minute_x = self.center_x + int(minute_length * math.cos(minute_angle))\n",
    "        minute_y = self.center_y + int(minute_length * math.sin(minute_angle))\n",
    "        draw.line((self.center_x, self.center_y, minute_x, minute_y), fill=255, width=2)\n",
    "\n",
    "        # Draw second hand\n",
    "        second_length = int(self.radius * 0.85)\n",
    "        second_x = self.center_x + int(second_length * math.cos(second_angle))\n",
    "        second_y = self.center_y + int(second_length * math.sin(second_angle))\n",
    "        draw.line((self.center_x, self.center_y, second_x, second_y), fill=255, width=1)\n",
    "\n",
    "        # Draw center dot\n",
    "        draw.ellipse(\n",
    "            (self.center_x - 2, self.center_y - 2,\n",
    "             self.center_x + 2, self.center_y + 2),\n",
    "            fill=255)\n",
    "\n",
    "        # Draw digital time on right side\n",
    "        time_str = now.strftime(\"%H:%M:%S\")\n",
    "        w = draw.textlength(time_str, font=self.font)\n",
    "        x = 70\n",
    "        y = (OLED_HEIGHT - self.font.size) // 2\n",
    "        draw.text((x, y), time_str, font=self.font, fill=255)\n",
    "\n",
    "        return image\n",
    "\n",
    "def main():\n",
    "    oled_init()\n",
    "    clock = ClockDisplay()\n",
    "\n",
    "    while True:\n",
    "        now = datetime.now()\n",
    "        image = clock.draw_hands(now)\n",
    "        display_image(image)\n",
    "        time.sleep(0.2)  # Update 5 times per second, adjust as needed\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
